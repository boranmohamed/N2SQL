# Qdrant Database Configuration
# Each application uses its own Qdrant database to avoid conflicts

# Main Application (default)
QDRANT_STORAGE_PATH=./qdrant_db_main

# Visual Tester (set in vanna_visual_tester.py)
# QDRANT_STORAGE_PATH=./qdrant_db_visual_tester

# Local Client (set in local_vanna_client.py)  
# QDRANT_STORAGE_PATH=./qdrant_db_local

# Vanna Configuration
USE_LOCAL_VANNA=true
LOCAL_VANNA_SERVER_URL=http://localhost:8001